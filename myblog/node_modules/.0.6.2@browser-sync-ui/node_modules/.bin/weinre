#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../.2.0.0-pre-I0Z7U9OV@weinre/weinre" "$@"
  ret=$?
else 
  node  "$basedir/../../../.2.0.0-pre-I0Z7U9OV@weinre/weinre" "$@"
  ret=$?
fi
exit $ret
